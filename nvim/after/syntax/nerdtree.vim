syntax match DevIconRed "\|\|" containedin=NERDTreeFlags
syntax match DevIconGreen "\|\|\|\|" containedin=NERDTreeFlags
syntax match DevIconYellow "\|\|\|\|\|λ\|\|\|\|\|" containedin=NERDTreeFlags
syntax match DevIconBlue "\|\|\|\|\|\|\|\|\|\|\|" containedin=NERDTreeFlags
syntax match DevIconMagenta "\|\|\|\|\|\|\|\|" containedin=NERDTreeFlags
